# Auto-gen the above constants
function Out-VirtualKeyCodes() {
	$html = Invoke-WebRequest -Uri "https://msdn.microsoft.com/en-us/library/windows/desktop/dd375731(v=vs.85).aspx"
	$html.ParsedHtml.getElementsByTagName("table") | ? { $_.hasAttribute("Responsive") } | % {
		$_.rows | % {
			$cnst = $_.getElementsByTagName("strong")[0].innerText
			$val = $_.getElementsByTagName("dt")[1].innerText
			$desc = $_.getElementsByTagName("p")[0].innerText
			if ($cnst -match "\w+") {
				$cmdlet = "Set-Variable {0} {1} -Option Constant # {2}" -f $cnst.PadRight(24), $val.Trim(), $desc.Trim()
				$cmdlet | Out-File VirtualKeyCodes.txt -Append
			}
		}
	}
}

